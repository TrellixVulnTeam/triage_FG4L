                                    <reporter username="jgar">Joao Garcia</reporter>
                        <labels>
                    </labels>
                <created>Mon, 9 Aug 2010 12:36:11 +0100</created>
                <updated>Thu, 12 Aug 2010 08:46:52 +0100</updated>
                            <resolved>Wed, 11 Aug 2010 22:30:34 +0100</resolved>
                                    <version>0.7</version>
