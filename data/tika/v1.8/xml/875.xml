org.apache.tika.parser.AutoDetectParser.parse(InputStream, ContentHandler, Metadata, ParseContext)&lt;br/&gt;
org.apache.tika.parser.DelegatingParser.parse(InputStream, ContentHandler, Metadata, ParseContext)&lt;br/&gt;
org.apache.tika.extractor.ParsingEmbeddedDocumentExtractor.parseEmbedded(InputStream, ContentHandler, Metadata, boolean)&lt;br/&gt;
org.apache.tika.parser.pkg.PackageExtractor.unpack(ArchiveInputStream, XHTMLContentHandler)&lt;br/&gt;
org.apache.tika.parser.pkg.PackageExtractor.parse(InputStream)&lt;br/&gt;
org.apache.tika.parser.pkg.PackageParser.parse(InputStream, ContentHandler, Metadata, ParseContext)&lt;br/&gt;
org.apache.tika.parser.CompositeParser.parse(InputStream, ContentHandler, Metadata, ParseContext)&lt;br/&gt;
