	&lt;li&gt;/tika/trunk/tika-parsers/src/main/java/org/apache/tika/parser/image/WebPParser.java&lt;/li&gt;
	&lt;li&gt;/tika/trunk/tika-parsers/src/main/resources/META-INF/services/org.apache.tika.parser.Parser&lt;/li&gt;
	&lt;li&gt;/tika/trunk/tika-parsers/src/test/java/org/apache/tika/parser/image/WebPParserTest.java&lt;/li&gt;
	&lt;li&gt;/tika/trunk/tika-parsers/src/test/resources/test-documents/testWebp_Alpha_Lossless.webp&lt;/li&gt;
	&lt;li&gt;/tika/trunk/tika-parsers/src/test/resources/test-documents/testWebp_Alpha_Lossy.webp&lt;/li&gt;
&lt;/ul&gt;
</comment>
