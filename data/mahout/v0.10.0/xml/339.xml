	at org.apache.mahout.clustering.meanshift.MeanShiftCanopyCreatorMapper.map(MeanShiftCanopyCreatorMapper.java:1)&lt;br/&gt;
	at org.apache.hadoop.mapred.MapRunner.run(MapRunner.java:50)&lt;br/&gt;
