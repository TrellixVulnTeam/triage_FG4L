&lt;ul class=&quot;alternate&quot; type=&quot;square&quot;&gt;
	&lt;li&gt;hints.ai_flags = AI_ADDRCONFIG;&lt;br/&gt;
+        //hints.ai_flags = AI_ADDRCONFIG;&lt;br/&gt;
