    &lt;span class=&quot;code-keyword&quot;&gt;public&lt;/span&gt; ClientConfiguration setZkTimeout(&lt;span class=&quot;code-object&quot;&gt;int&lt;/span&gt; zkTimeout) {
        setProperty(ZK_SERVERS, &lt;span class=&quot;code-object&quot;&gt;Integer&lt;/span&gt;.toString(zkTimeout));
        &lt;span class=&quot;code-keyword&quot;&gt;return&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;this&lt;/span&gt;;
    }
&lt;/pre&gt;
